{
  "name": "project-03",
  "version": "1.0.0",
  "description": "Gympass",
  "main": "server.js",
  "scripts": {
    "build": "tsup src --out-dir build",
    "production": "node build/server.js",
    "server": "tsx watch src/server.ts",
    "test:create-prisma-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "test": "vitest run --dir ./src/services",
    "test:e2e": "vitest --dir src/http/controllers/",
    "pretest:e2e": "run-s test:create-prisma-env test:install-prisma-env",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "Nodejs-Basics"
  },
  "author": "Tom Alexander",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "^18.16.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "eslint": "8.39.0",
    "npm-run-all": "4.1.5",
    "prisma": "4.13.0",
    "supertest": "6.3.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/jwt": "6.7.1",
    "@prisma/client": "4.13.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "^4.17.0",
    "zod": "3.21.4"
  }
}
